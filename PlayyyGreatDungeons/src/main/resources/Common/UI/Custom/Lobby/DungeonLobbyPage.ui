$C = "../Common.ui";
Group {
  LayoutMode: Top;
  Background: (Color: #0c1d2c);
  Anchor: (Width: 1200, Height: 720);
  Padding: (Full: 8);
  Group #Title {
    LayoutMode: MiddleCenter;
    Background: (Color: #8d12ba);
    FlexWeight: 0;
    Padding: (Full: 4);
    Label #Label30ae39cd {
      Background: (Color: #33333333);
      FlexWeight: 1;
      Text: "Great Dungeons";
      Style: (FontSize: 14, Alignment: Center, TextColor: #ffffff, RenderBold: true);
    }
  }
  Group #Content {
    LayoutMode: Left;
    Background: (Color: #0e2048);
    FlexWeight: 1;
    Padding: (Full: 8);
    Group #LeftContainer {
      LayoutMode: Top;
      FlexWeight: 1;
      Group #Lobby {
        LayoutMode: Top;
        Label #Labelfddac357 {
          Text: "Dungeon Lobby ID:";
          Style: (TextColor: #ffffff);
        }
      }
    }
    Group {
      LayoutMode: Top;
      Anchor: (Right: 10);
    }
    Group #MiddleContainer {
      LayoutMode: Top;
      FlexWeight: 1;
      Label {
        Text: "Players";
        Style: (TextColor: #ffffff);
      }
      Group {
        LayoutMode: Top;
        Anchor: (Bottom: 2);
      }
      Group #PlayerList {
        LayoutMode: TopScrolling;
        ScrollbarStyle: $C.@DefaultScrollbarStyle;
      }
      $C.@TextButton #ReadyButton {
        Padding: (Full: 0);
        Text: "Ready";
      }
    }
    Group {
      LayoutMode: Top;
      Anchor: (Right: 10);
    }
    Group #RightContainer {
      FlexWeight: 1;
      Label {
        Text: "Dungeon Info";
        Style: (TextColor: #ffffff);
      }
    }
